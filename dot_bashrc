#
# ~/.bashrc
#

export GOPATH=$HOME/go
# If not running interactively, don't do anything
[[ $- != *i* ]] && return

alias grep='grep --color=auto'
PS1='[\u@\h \W]\$ '
alias ls="lsd"
alias ll="ls -l"
alias la="ls -lAtr"
alias cat="bat"
export PATH="$PATH:$HOME/go/bin"
eval "$(starship init bash)"
